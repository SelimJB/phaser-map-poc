{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": [
			"dom",
			"ESNext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": true,
		"noUnusedLocals": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"resolveJsonModule": true,
		"downlevelIteration": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"types": [
			"vite/client"
		],
		"isolatedModules": true,
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./src",
		"paths": {
			"@/*": [
				"*"
			],
			"@phaser/*": [
				"phaser/*"
			],
			"@map": [
				"map/*"
			]
		},
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/**/*"
	]
}